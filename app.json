{
  "expo": {
    "name": "Zaf's Kitchen",
    "slug": "zaf-kitchen",
    "scheme": "zaf-kitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Zafs.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Zafs.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.zafkitchen.staffapp",
      "icon":"./assets/Zafs.png",
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "zaf-kitchen",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-asset",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8e1b4ac4-0dcd-4da1-be34-ca9430a8886d"
      }
    },
    "experiments": {
      "typedRoutes": false
    }
  }
}
